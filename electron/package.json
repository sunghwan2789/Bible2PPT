{
  "name": "bible2ppt",
  "version": "2.0.0",
  "author": {
    "name": "Sunghwan Bang"
  },
  "license": "MIT",
  "main": "main.js",
  "private": true,
  "scripts": {
    "parcel:build": "parcel build src/index.html --no-source-maps --target electron --out-dir build --public-url ./",
    "parcel:watch": "parcel watch src/index.html --target electron --out-dir build --public-url ./",
    "clean": "rm -rf .cache/ build/ dist/",
    "start": "concurrently \"npm:parcel:watch\" \"electron .\""
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "concurrently": "^5.0.2",
    "electron": "^7.1.7",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunghwan2789/Bible2PPT.git"
  }
}
